@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityUser>
<h2>List of Users</h2>
<hr />
<table class="table table-striped">
<tr>
<th>Email</th>
<th></th>
<th></th>
</tr>
@foreach(var item in Model)
{
<tr>
<td>@item.UserName</td>
<td>
<form
action="@Url.Action("ManageUserRoles","RolesAdmin")" method="post"
class="form-inline">
<input type="hidden" id="UserName" name="UserName" value="@item.UserName"/>
<input type="submit" class="btn btn-primary" value="Manage Roles"/>
</form>
</td>
<td>
@Html.ActionLink("ResetPassword","resetpassword",new{id=item.Id},new{@class="btn btn-primary"})
</td>
</tr>
}
</table>